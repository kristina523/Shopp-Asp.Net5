@model Shopp.Models.Brand

@{
    ViewBag.Title = Model.Names;
}

<link rel="stylesheet" href="~/css/brandDetails.css" />

<div class="brand-details">
    <h2>Бренд @Model.Names</h2>
    <dl class="dl-horizontal">
        <dt>Идентификатор</dt>
        <dd>@Model.BrandsId</dd>

        <dt>Название</dt>
        <dd>@Model.Names</dd>

        <dt>Страна</dt>
        <dd>@Model.Country</dd>
    </dl>
</div>